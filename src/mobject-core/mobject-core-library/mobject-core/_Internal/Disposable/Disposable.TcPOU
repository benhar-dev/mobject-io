	instancePathUsedToCheckIfDynamic : T_MAXSTRING;
	referenceCount : UDINT := 1;
END_VAR]]></Declaration>
    <Implementation>
      <ST><![CDATA[]]></ST>
    </Implementation>
    <Folder Name="Protected" Id="{4c53547f-9172-4827-abf4-7bbd1b7af9dc}" />
    <Method Name="AddReference" Id="{48439e69-8098-49ea-990b-a1612fb609e6}">
      <Declaration><![CDATA[METHOD AddReference
VAR_INPUT
END_VAR
VAR
	debug : UDINT;
END_VAR]]></Declaration>
      <Implementation>
        <ST><![CDATA[referenceCount := referenceCount + 1;

