      </Implementation>
    </Method>
    <Method Name="OnDispose" Id="{557c0ee7-c9e8-4533-927d-e6ba6353bbd0}" FolderPath="Protected\">
      <Declaration><![CDATA[METHOD PROTECTED OnDispose
